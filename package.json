{
  "name": "@altus/node-env",
  "title": "@altus/node-env",
  "version": "1.0.0",
  "description": "Node environment module",
  "main": "dist/release/index.js",
  "typings": "./index.d.ts",
  "engine": {
    "node": ">= 0.8.0"
  },
  "author": {
    "name": "Altus Carstens",
    "url": "http://dieheld.co.za"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/carstensaltus/node-env.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "watch": "./node_modules/.bin/gulp watch",
    "test": "npm run coverage",
    "coverage": "npm run build; istanbul cover ./node_modules/.bin/_mocha -- -- -u export -R spec ./dist/test/**/*.test.js"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "allow-untracked-files": false,
      "pre-commit": [
        "npm test"
      ],
      "post-commit": "git status",
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "npm test"
      ],
      "post-checkout": "npm install",
      "post-merge": "npm install"
    }
  },
  "keywords": [
    "javascript",
    "node",
    "helper",
    "environment",
    "variable",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/carstensaltus/node-env/issues"
  },
  "homepage": "https://github.com/carstensaltus/node-env#readme",
  "files": [
    "./dist"
  ],
  "devDependencies": {
    "@types/chai": "4.0",
    "@types/mocha": "2.2",
    "@types/node": "4.2",
    "chai": "4.0",
    "gulp": "3.9",
    "gulp-clean": "0.3",
    "gulp-concat": "2.6",
    "gulp-documentation": "3.2",
    "gulp-istanbul": "1.1",
    "gulp-tslint": "8.1",
    "gulp-typescript": "3.1",
    "istanbul": "0.4",
    "merge2": "1.0",
    "mocha": "3.4",
    "pre-git": "3.15",
    "run-sequence": "1.2",
    "tslint": "5.4",
    "typescript": "2.3"
  },
  "dependencies": {}
}
